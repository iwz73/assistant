<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<on-start>
		<evaluate expression="basicFlowController.generateBasicFlowVo()" result="flowScope.basicFlowVo"/>
	</on-start>
	
	<view-state id="basicFlow_0" view="/WEB-INF/jsp/basicFlow/basicFlow_0.jsp" model="flowScope.basicFlowVo">
		<transition on="next" to="basicFlow_0"></transition>
		<transition on="cancel" to="basicFlow_0"></transition>
	</view-state>

</flow>
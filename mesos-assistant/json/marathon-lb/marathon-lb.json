{
	"id": "marathon-lb",
	"instances": 1,
	"constraints": [["hostname", "UNIQUE"]],
	"container": {
		"type": "DOCKER",
		"docker": {
			"image": "docker.io/mesosphere/marathon-lb",
			"privileged": true,
			"network": "HOST"
		}
	},
	"args": ["sse", "-m","http://127.0.0.1:8080","--group", "external"]
}
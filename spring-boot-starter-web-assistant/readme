# run in docker
su -
/home/thank/git/assistant/spring-boot-starter-web-assistant/mvnw install dockerfile:build
docker login -u <<USERNAME>> -p <<PASSWORD>>
/home/thank/git/assistant/spring-boot-starter-web-assistant/mvnw install dockerfile:push
docker run -e "SPRING_PROFILES_ACTIVE=prod" --net="host" -p 8080:8080 -t hsiehpinghan/spring-boot-starter-web-assistant
docker ps -all
docker kill <<CONTAINER ID>>
docker rm <<CONTAINER ID>>
docker images
docker rmi <<IMAGE ID>>

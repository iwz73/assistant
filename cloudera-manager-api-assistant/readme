curl -X GET -u admin:admin http://127.0.0.1:7180/api/v14/clusters
curl -X GET -u admin:admin http://127.0.0.1:7180/api/v14/clusters/Cluster%201/services
curl -X GET -u admin:admin http://127.0.0.1:7180/api/v14/clusters/Cluster%201/services/hdfs/config
curl -X GET -u admin:admin http://127.0.0.1:7180/api/v14/clusters/Cluster%201/services/hdfs/config?view=full
curl -X GET -u admin:admin http://127.0.0.1:7180/api/v14/timeseries?query=select%20dfs_capacity,%20dfs_capacity_used,%20dfs_capacity_used_non_hdfs%20where%20entityName=hdfs

